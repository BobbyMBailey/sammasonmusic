{
  "name": "sammasonmusic",
  "version": "2.0.0",
  "description": "Sam Mason Music v2",
  "main": "index.js",
  "repository": "https://johntimothybailey@github.com/BobbyMBailey/sammasonmusic.git",
  "author": "Robert Bailey",
  "license": "MIT",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test": "yarn lint && yarn test:unit",
    "test:unit": "vue-cli-service test",
    "e2e": "concurrently --success=first --kill-others 'yarn build && yarn start' 'yarn e2e:run' ",
    "e2e:run": "wait-on http://localhost:3000 && vue-cli-service e2e --url=http://localhost:3000/",
    "e2e:stream": "wait-on http://localhost:3000 && vue-cli-service e2e --url=http://localhost:3000/ --record --key 37fded90-79ad-45de-a7ab-d989e2eee7b0",
    "e2e:open": "vue-cli-service e2e:open --url=http://localhost:3000/",
    "lint": "eslint --ext .js,.vue . && stylelint assets/**/*.scss **/*.vue --syntax scss",
    "lint:fix": "eslint --fix --ext .js,.vue . && stylelint assets/**/*.scss --syntax scss --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-brands": "^5.0.9",
    "@fortawesome/fontawesome-free-solid": "^5.0.9",
    "@fortawesome/vue-fontawesome": "^0.0.22",
    "@material/button": "^0.34.1",
    "@material/theme": "^0.34.0",
    "nuxt": "^1.0.0",
    "nuxt-fontawesome": "^0.1.3",
    "nuxt-seo-module": "^1.0.0",
    "vue-bem-cn": "^1.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-e2e-cypress": "^3.0.0-beta.6",
    "@vue/cli-plugin-unit-mocha": "^3.0.0-beta.6",
    "@vue/cli-service": "^3.0.0-beta.6",
    "@vue/eslint-config-standard": "^3.0.0-beta.6",
    "@vue/test-utils": "^1.0.0-beta.13",
    "babel-eslint": "^8.2.1",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-vue": "^4.0.0",
    "modularscale-sass": "^3.0.5",
    "node-sass": "^4.8.3",
    "sass-loader": "^7.0.1",
    "standard": "^11.0.1",
    "stylelint": "^9.1.3",
    "stylelint-config-sass-guidelines": "^5.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-processor-html": "^1.0.0",
    "vue-loader": "^14.2.2",
    "vue-style-loader": "^4.1.0",
    "wait-on": "^2.1.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
